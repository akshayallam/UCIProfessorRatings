{
  "manifest_version": 2,

  "name": "UCI Professor Ratings",
  "description": "Quickly access the professor's RateMyProfessor page from the Schedule of Classes page with just a few clicks",
  "version": "1.5",
  "icons": {
    "16": "images/final-16.png",
    "32": "images/final-32.png",
    "48": "images/final-48.png",
    "128": "images/final-128.png"
  },

  "background": {
    "scripts": ["eventPage.js"]
  },

  "browser_action": {
    "default_icon": "images/final-128.png"
  },

  "permissions": [        
          "contextMenus"
        ]
}